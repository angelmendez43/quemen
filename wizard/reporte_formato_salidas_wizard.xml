<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="view_quemen_formato_salidas_wizard" model="ir.ui.view">
              <field name="name">view.quemen.formato_salidas.wizard.</field>
              <field name="model">quemen.reporte_formato_salidas.wizard</field>
              <field name="arch" type="xml">
                <form string="Reporte de existencias">
                    <group>
                        <field name="fecha_desde"/>
                        <field name="fecha_hasta"/>
                    </group>
                    <footer>
                        <!-- <button name="print_report" string="Print"  type="object" class="btn-primary"/> -->
                        <button name="print_report" string="Generar reporte" type="object" class="oe_highlight"/>
                        <button string="Cancel" class="btn-default" special="cancel" />
                    </footer>
                </form>
              </field>
        </record>



        <act_window id="quemen_action_reporte_formato_salidas"
            name="Reporte de formato de salidas"
            res_model="quemen.reporte_formato_salidas.wizard"
            binding_model="pos.order"
            view_mode="form" target="new"/>

