<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="view_quemen_reporte_codigo_barras_wizard" model="ir.ui.view">
              <field name="name">view.quemen.codigo.barras.wizard.</field>
              <field name="model">quemen.reporte_codigo_barras.wizard</field>
              <field name="arch" type="xml">
                <form string="Codigos de barra">
                    <group>
                        <field name="product_ids">
                            <tree string="Productos" editable="bottom">
                                <field name="product_id"/>
                                <field name="quantity" readonly="True"/>
                                <field name="qty_label"/>
                            </tree>
                        </field>
                    </group>
                    <footer>
                        <!-- <button name="print_report" string="Print"  type="object" class="btn-primary"/> -->
                        <button name="print_report" string="Generar codigos de barra" type="object" class="oe_highlight"/>
                        <button string="Cancel" class="btn-default" special="cancel" />
                    </footer>
                </form>
              </field>
        </record>



        <record id="action_quemen_reporte_codigo_barras" model="ir.actions.act_window">
            <field name="name">Codigo de barras</field>
            <field name="res_model">quemen.reporte_codigo_barras.wizard</field>
            <field name="view_mode">form</field>
            <field name="view_id" ref="view_quemen_reporte_codigo_barras_wizard"/>
            <field name="target">new</field>
            <field name="binding_model_id" ref="model_quemen_op_lote"/>
            <!-- <field name="paperformat_id" ref="quemen.paperformat_ticket"/> -->
        </record>

            <!-- <act_window id="window_planilla" name="Planilla" res_model="hr_gt.planilla.wizard" src_model="account.journal" view_mode="form" target="new"/> -->


